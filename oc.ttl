oce:SchemaOrg a schema:Website, owl:Ontology;
    schema:name "Schema.org" ;
    schema:description "Schema.org is a collaborative, community activity with a mission to create, maintain, and promote schemas for structured data on the Internet, on web pages, in email messages, and beyond.";
    schema:url "https://semantify.it/" ;
    schema:datePublished "2011-01-01"^^xsd:date; #dct:issued
    schema:creator "Google", "Yahoo!", "Bing", "Yandex". # do orgs instead if used elsewhere

oce:DBpediaOntology a owl:Ontology;
    schema:name "DBpedia Ontology" ;
    schema:numClasses "760"^^xsd:integer;
    schema:numProperties "3000"^^xsd:integer.

oce:Wikipedia a schema:WebSite;
    schema:url "http://wikipedia.org".

oce:DBpedia a oc:OpenKnowledgeGraph ;
    schema:publicAccess "true"^^xsd:boolean ;
    schema:name "Dbpedia" ;
    schema:description "It is the de-facto central dataset on the semantic web (also referred as “nucleus”) since it is linked to many other datasets." ;
    schema:accessibilityControl "RDF Dumps", "Sparql Endpoints";
    schema:datePublished "2007-01-01"^^xsd:date;
    oce:numTripples "13 000 000 000"^^xsd:integer;
    schema:isBasedOn oce:DBpediaOntology, oce:Wikipedia; # maybe split into 2 diff props
    schema:educationalUse "Natural Language Processing", "knowledge exploration and enrichment".

oce:Freebase a oc:OpenKnowledgeGraph ;
    schema:publicAccess "true"^^xsd:boolean ;
    schema:name "Freebase";
    schema:datePublished "2007-01-01"^^xsd:date;
    oce:numTripples "1 900 000 000"^^xsd:integer.
    #shut down on 2016

oce:YAGO a oc:OpenKnowledgeGraph ;
    schema:publicAccess "true"^^xsd:boolean ;
    schema:name "YAGO";
    schema:description "YAGO is a Knowledge Graph built based on Wikipedia content.";
    schema:datePublished "2008-01-01"^^xsd:date;
    oce:numTripples "120 000 000"^^xsd:integer. # 120M facts and 350K classes

oce:Cyc a oc:ProprietaryKnowledgeGraph ;
    schema:publicAccess "false"^^xsd:boolean ;
    schema:name "Cyc";
    schema:datePublished "1984-01-01"^^xsd:date;
    oce:numTripples "20 000 000 000"^^xsd:integer. # says axioms, is same as tripples?
    #1.5m concepts?

oce:FacebooksEntitiesGraph a oc:ProprietaryKnowledgeGraph ;
    schema:publicAccess "true"^^xsd:boolean ; # accessible via the Facebook Graph API?!
    schema:name "FacebooksEntitiesGraph";
    schema:description "Facebook's Entities Graph is maintained by Facebook and used internally to support the graph search functionality."
    schema:datePublished "2010-01-01"^^xsd:date;
    oce:numTripples "500 000 000 000"^^xsd:integer.

oce:SemantifyIt a schema:WebApplication, schema:WebSite ;
    schema:name "semantify.it" ;
    schema:url "https://semantify.it/" ;
    schema:description "Semantify.it gives your website the power to be perfectly understandable for search engines, intelligent assistants and chatbots by using schema.org annotations. Semantify.it gives you easy-to-use annotation creation and deployment.";
    schema:softwareAddOn oce:smtfyAnnotationEditor, oce:smtfyEvaluator.

oce:smtfyEvaluator a schema:WebApplication;
    schema:name "Semantify.it Evaluator";
    schema:softwareAddOn oce:smtfyValidator, oce:smtfyVerificator.

oce:smtfyValidator a schema:WebApplication;
    schema:about oce:Validation;
    schema:screenshot oce:VerificationReportScreenShot, oce:VerificationReport2ScreenShot.

oce:smtfyVerificator a schema:WebApplication;
    schema:about oce:Verification.

oce:smtfyAnnotationEditor a schema:WebApplication;
    schema:name "Annotation Editor";
    schema:screenshot oce:AnnotationEditorScreenShot;
    schema:hasPart [
        a schema:HowTo;
        schema:name "Create an Annotation";
        schema:yied oce:Annotation;
        schema:step: [
            a schema:HowToStep;
            schema:position "1"^^xsd:integer;
            schema:text "Select a domain specification"
        ], [
            a schema:HowToStep;
            schema:position "2"^^xsd:integer;
            schema:text "Fill in required fields"
        ];
    ] .

oce:Annotation a schema:CreativeWork;
    schema:name "JsonLD Annotataion";
    schema:encodingFormat "application/ld+json".


