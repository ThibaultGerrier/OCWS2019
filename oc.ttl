##### VOCAB


schema:publicAccess schema:domainIncludes oc:KnowledgeGraph .

oc:numTripples a rdf:Property ;
    schema:name "numTripples" ;
    schema:domainIncludes oc:KnowledgeGraph ;
    schema:rangeIncludes schema:Number;
    rdfs:comment "The number of tripples in a knowledge graph" .

oc:numClasses a rdf:Property ;
    schema:name "numClasses" ;
    schema:domainIncludes owl:Ontology ;
    schema:rangeIncludes schema:Number;
    rdfs:comment "The number of classes in an ontology" .

oc:numProperties a rdf:Property ;
    schema:name "numProperties" ;
    schema:domainIncludes owl:Ontology ;
    schema:rangeIncludes schema:Number;
    rdfs:comment "The number of properties in an ontology" .

oc:understand a rdf:Property ;
    schema:name "understanding" ;
    schema:domainIncludes oc:Concept ;
    schema:rangeIncludes oc:Concept;
    rdfs:comment "The ability to understand something; comprehension and the power of abstract thought; intellect." .

oc:utilizes a rdf:Property ;
    schema:name "utilizes" ;
    schema:domainIncludes oc:Concept ;
    schema:rangeIncludes oc:Concept;
    rdfs:comment "Make practical and effective use of." .

   ### CLASS

oc:KnowledgeGraph a rdfs:Class ;
    schema:name "KnowledgeGraph" ;
    rdfs:comment "Knowledge Graphs are very large semantic nets that integrate various and heterogeneous information sources to represent knowledge about certain domains of discourse" ;
    rdfs:subClassOf schema:Dataset .

# TODO add different types of comments / definitions --> Empirical (Chapter 1.3), Conceptual (Chapter 1.2)

oc:OpenKnowledgeGraph a rdfs:Class ;
    schema:name "OpenKnowledgeGraph" ;
    rdfs:comment "The research on semantic web and linked data led to many open datasets eventually to comprise the Linked Open Data cloud. These datasets are now mostly rebranded as Knowledge Graphs." ;
    rdfs:subClassOf oc:KnowledgeGraph .

oc:ProprietaryKnowledgeGraph a rdfs:Class ;
    schema:name "ProprietaryKnowledgeGraph" ;
    rdfs:comment "Various Knowledge Graphs have been developed by companies to enable their customers' applications." ;
    rdfs:subClassOf oc:KnowledgeGraph .

oc:Concept a rdfs:Class ;
    schema:name "Concept" ;
    rdfs:comment "A scientific concept is an idea or model. explaining some natural phenomenon." ;
    rdfs:subClassOf schema:CreativeWork, skos:Concept.


